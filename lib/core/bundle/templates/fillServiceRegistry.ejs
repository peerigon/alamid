"use strict";

var serviceRegistry = require(<%= JSON.stringify(rootPath) %> + "/node_modules/alamid/lib/shared/registries/serviceRegistry.js");
var ServiceClasses = [];

<% _(services).each(function eachService(servicePath, serviceUrl)  { %>
ServiceClasses[<%= JSON.stringify(serviceUrl) %>] = require(<%= JSON.stringify(servicePath) %>);
serviceRegistry.setService(<%= JSON.stringify(serviceUrl) %>, new ServiceClasses[<%= JSON.stringify(serviceUrl) %>]());
<% }); %>